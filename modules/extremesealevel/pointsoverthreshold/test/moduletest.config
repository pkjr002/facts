PIPELINE_ID=extremesealevel-pointsoverthreshold-extremesealevel-SSP585
MINDAYS=250
MINYEARS=20
MATCH_LIM=1
CENTER_YEAR=2005
PCTPOT=95
GPD_POT_THRESHOLD=99.7
CLUSTER_LIM=72
TOTAL_LOCALSL_FILE=test/data/total-workflow-localsl.nc
TARGET_YEARS=2050,2100
GESLA_DIR=gesla_data
STATION_DATA_FILE="$PIPELINE_ID_station_data.pkl"
ESL_FIT_FILE="$PIPELINE_ID_fit.pkl"
