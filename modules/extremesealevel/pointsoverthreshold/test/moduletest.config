STAGEOPTIONS[2]="--pipeline_id $PIPELINE_ID --station_data_file $STATION_DATA_FILE"
STAGEOPTIONS[3]="--pipeline_id $PIPELINE_ID --esl_fit_file $ESL_FIT_FILE --slproj_file $SLPROJ_FILE --min_z $MIN_Z --max_z $MAX_Z --z_step $Z_STEP --allowance_freq $ALLOWANCE_FREQ --nsamps $NSAMPS --seed $SEED"

source $FACTSSCRIPT_DIR/moduletest/moduletest.parsestages.sh
