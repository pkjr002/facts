PIPELINE_ID=icesheets-dp21-icesheet-$SCENARIO
SCENARIO=$RCPATHWAY
BASEYEAR=2005
PYEAR_START=2020
PYEAR_END=2300
PYEAR_STEP=10
SEED=4321
CHUNKSIZE=662

STAGES=( "pre-process" "fit" "project" "post-process" )
STAGEOPTIONS[1]="--pipeline_id $PIPELINE_ID --scenario $SCENARIO --baseyear $BASEYEAR"
STAGEOPTIONS[2]="--pipeline_id $PIPELINE_ID"
STAGEOPTIONS[3]="--pipeline_id $PIPELINE_ID --nsamps $NSAMPS --pyear_start $PYEAR_START --pyear_end $PYEAR_END --pyear_step $PYEAR_STEP --seed $SEED"
STAGEOPTIONS[4]="--pipeline_id $PIPELINE_ID --chunksize $CHUNKSIZE --locationfile $LFILE"

source $FACTSSCRIPT_DIR/moduletest/moduletest.parsestages.sh
